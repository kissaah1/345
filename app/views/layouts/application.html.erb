<!doctype html><!-- HTML 5 -->
<html lang="en">
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<meta name="author" content="Eudaimonia Inc.">
	<meta name="description" content="Preparing organizations and individuals to thrive and sucseed.">
	<meta name="keywords" content="Employee success, Career Design, Human Resouce, key word, key word, key word, key word, key word, key word, key word, key word">
	<meta name="robots" content="all">
	<title><%= full_title(yield(:title)) %></title>
	<link href="https://fonts.googleapis.com/icon?family=Material+Icons"
			rel="stylesheet">
	<%= favicon_link_tag 'favicon.ico' %>
	<%= stylesheet_link_tag		'application', media: 'all', 'data-turbolinks-track' => true %>
	<%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
	<%= csrf_meta_tags %>
	<%= render 'layouts/shim' %>
</head>

<% if logged_in? %>
<body id="logged-in-view">
<% else %>
<body id="public-view">
<% end %>
<div id="container">
<%= debug(params) if Rails.env.development? %>
<%= render 'layouts/header' %>
<div class="bottomfix wrap">
	<% flash.each do |message_type, message| %>
		<div class="row">
		<div class="columns grid-80 centered clearfix">

		<div class="alert alert-<%= message_type %>">
		<small>
		<%= message %>
		</small>
		</div>
		</div>
		</div>
	<% end %>

<%= yield %>
</div><!-- bottomfix -->
<%= render 'layouts/footer' %>
</div><!-- container -->

<%# if logged_in? %>
<%#= render 'joyride' %>
<%# end %>

<%= render 'dropdown' %>
</body>
</html>
