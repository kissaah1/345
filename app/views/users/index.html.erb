<% provide(:title, "All Users") %>
<section class="centered bottomfix">
	<div class="row">
		<div class="columns grid-80 centered clearfix">
		<h2>All Users</h2>
		<%= will_paginate %>
		<ul class="users">
		<% @users.each do |user| %>
		<%= render user %>
		<% end %>
		</ul>
		<%= will_paginate %>
		</div><!-- columns -->
	</div><!-- row -->
</section><!-- bottomfix -->
